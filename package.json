{
  "name": "backed-community-nft",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:api:dev": "npx dotenv -e .env.development -- npx ts-node api/src/index.ts",
    "start:api:prod": "npx ts-node api/src/index.ts",
    "test:api": "jest __tests__/api --runInBand",
    "reset:db:api": "docker-compose down && docker volume rm $(docker volume ls -q)",
    "prisma:studio:local": "dotenv -e .env.local -- npx prisma studio --schema api/prisma/schema.prisma",
    "prisma:studio:dev": "dotenv -e .env.development -- npx prisma studio --schema api/prisma/schema.prisma",
    "prisma:studio:prod": "dotenv -e .env.production.local -- npx prisma studio --schema api/prisma/schema.prisma",
    "prisma:migrate:local": "dotenv -e .env.local -- npx prisma migrate dev --schema api/prisma/schema.prisma",
    "prisma:migrate:dev": "dotenv -e .env.development -- npx prisma migrate deploy --schema api/prisma/schema.prisma",
    "prisma:migrate:prod": "dotenv -e .env.production.local -- npx prisma migrate deploy --schema api/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema api/prisma/schema.prisma",
    "svg:reload": "node svg-reload/index.js",
    "run:script:local": "dotenv -e .env.local -- ts-node",
    "run:script:dev": "dotenv -e .env.development -- ts-node"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.2",
    "@ethereumjs/vm": "5.8.0",
    "@ethersproject/abi": "5.6.0",
    "@gnosis.pm/safe-core-sdk": "^2.1.0",
    "@gnosis.pm/safe-core-sdk-types": "^1.1.0",
    "@gnosis.pm/safe-ethers-lib": "^1.1.0",
    "@gnosis.pm/safe-service-client": "^1.1.2",
    "@noble/hashes": "^1.0.0",
    "@pinata/sdk": "^1.1.25",
    "@prisma/client": "^3.14.0",
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.2",
    "body-parser": "^1.20.0",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.3",
    "discord.js": "12.5.3",
    "eip-712": "^1.0.0",
    "eth-typed-data": "^0.1.0-beta.0",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.6.7",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "prisma": "^3.14.0",
    "solc": "0.8.12",
    "xmldom": "0.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "5.0.0",
    "jest": "^28.1.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  }
}
